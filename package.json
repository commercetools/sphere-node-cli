{
  "name": "sphere-node-cli",
  "description": "A fast, performant, easy-to-use and stream-based CLI for SPHERE.IO",
  "version": "0.0.1",
  "preferGlobal": true,
  "private": true,
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "bin": {
    "sphere": "./bin/sphere"
  },
  "scripts": {
    "build": "coffee --compile --output lib src",
    "test": "npm run build && jasmine-node --captureExceptions --coffee test"
  },
  "main": "./lib/sphere",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-cli/issues"
  },
  "keywords": [
    "cli",
    "sphere",
    "sphere.io",
    "stream",
    "import",
    "export"
  ],
  "engines": {
    "node": ">= 0.10.x"
  },
  "dependencies": {
    "commander": "2.6.0",
    "debug": "2.1.x",
    "sphere-node-sdk": "1.2.0"
  },
  "devDependencies": {
    "coffee-script": "1.9.x",
    "jasmine-node": "1.14.5",
    "rewire": "2.3.x",
    "should": "5.0.x"
  },
  "licenses": "MIT"
}