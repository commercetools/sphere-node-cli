{
  "name": "sphere-node-cli",
  "description": "A fast, performant, easy-to-use and stream-based CLI for SPHERE.IO",
  "version": "0.5.0",
  "preferGlobal": true,
  "private": false,
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "bin": {
    "sphere": "./bin/sphere",
    "sphere-fetch": "./bin/sphere-fetch",
    "sphere-export": "./bin/sphere-export",
    "sphere-import": "./bin/sphere-import"
  },
  "scripts": {
    "build": "npm run lint && npm run clean && babel --optional runtime lib --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint lib test",
    "release": "mversion patch -m",
    "release:patch": "mversion patch -m",
    "release:minor": "mversion minor -m",
    "release:major": "mversion major -m",
    "test": "npm run lint && NODE_ENV=test mocha --compilers js:babel/register --recursive test/*.spec.js test/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:features": "npm run build && babel-node $(npm bin)/cucumber.js"
  },
  "main": "./dist/sphere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-cli/issues"
  },
  "keywords": [
    "cli",
    "sphere",
    "sphere.io",
    "stream",
    "import",
    "export"
  ],
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {
    "JSONStream": "1.0.7",
    "babel-runtime": "5.8.20",
    "commander": "emmenko/commander.js#patch-help",
    "ct-customer-import": "0.4.0",
    "debug": "2.2.0",
    "highland": "2.5.1",
    "inflection": "1.8.0",
    "sphere-category-sync": "~0.2.6",
    "sphere-node-sdk": "~1.8.1",
    "sphere-node-utils": "0.7.0",
    "sphere-product-import": "~0.5.14",
    "sphere-product-type-import": "1.0.0",
    "sphere-stock-import": "~0.7.5",
    "stream-transform": "0.1.1",
    "winston": "1.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "cucumber": "^0.9.2",
    "eslint": "^1.10.3",
    "eslint-config-commercetools": "^0.2.1",
    "expect": "^1.13.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "mversion": "^1.10.1",
    "rewire": "^2.5.1",
    "rimraf": "^2.4.4",
    "should": "^7.1.0",
    "sinon": "^1.17.2"
  }
}
