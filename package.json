{
  "name": "sphere-node-cli",
  "description": "A fast, performant, easy-to-use and stream-based CLI for SPHERE.IO",
  "version": "0.2.1",
  "preferGlobal": true,
  "private": false,
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "bin": {
    "sphere": "./bin/sphere",
    "sphere-fetch": "./bin/sphere-fetch",
    "sphere-export": "./bin/sphere-export",
    "sphere-import": "./bin/sphere-import"
  },
  "scripts": {
    "build": "npm run lint && coffee --compile --output lib src",
    "clean": "rimraf lib",
    "lint": "coffeelint -f ./node_modules/sphere-coffeelint/coffeelint.json src test",
    "release": "mversion patch -m",
    "release:patch": "mversion patch -m",
    "release:minor": "mversion minor -m",
    "release:major": "mversion major -m",
    "test": "npm run build && jasmine-node --coffee test",
    "test:watch": "npm run test -- --autoTest --watchFolders .",
    "test:features": "npm run lint -- features && cucumber.js"
  },
  "main": "./lib/sphere",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-cli/issues"
  },
  "keywords": [
    "cli",
    "sphere",
    "sphere.io",
    "stream",
    "import",
    "export"
  ],
  "engines": {
    "node": ">= 0.10.x"
  },
  "dependencies": {
    "bluebird": "2.9.33",
    "commander": "emmenko/commander.js#master",
    "debug": "2.2.0",
    "highland": "2.5.1",
    "JSONStream": "1.0.4",
    "sphere-node-sdk": "1.4.1",
    "sphere-node-utils": "0.7.0",
    "sphere-stock-import": "0.7.3",
    "sphere-product-import": "0.5.1",
    "stream-transform": "0.1.0",
    "underscore": "1.8.3",
    "underscore-mixins": "0.1.4",
    "underscore.inflection": "1.2.0",
    "winston": "1.0.1"
  },
  "devDependencies": {
    "coffeelint": "1.10.1",
    "coffee-script": "1.9.3",
    "cucumber": "0.5.2",
    "jasmine-node": "2.0.0",
    "mkdirp": "0.5.1",
    "mversion": "1.10.0",
    "rewire": "2.3.4",
    "rimraf": "2.4.1",
    "should": "7.0.2",
    "sphere-coffeelint": "sphereio/sphere-coffeelint#master"
  },
  "licenses": "MIT"
}