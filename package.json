{
  "name": "sphere-node-cli",
  "description": "A fast, performant, easy-to-use and stream-based CLI for SPHERE.IO",
  "version": "0.6.0",
  "preferGlobal": true,
  "private": false,
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "bin": {
    "sphere": "./bin/sphere",
    "sphere-import": "./bin/sphere-import"
  },
  "scripts": {
    "build": "npm run lint && npm run clean && babel --optional runtime lib --out-dir dist",
    "clean": "rimraf dist coverage",
    "commit": "git-cz",
    "lint": "./scripts/lint lib test",
    "release": "mversion patch -m",
    "release:patch": "mversion patch -m",
    "release:minor": "mversion minor -m",
    "release:major": "mversion major -m",
    "test": "npm run lint && cross-env NODE_ENV=test babel-node node_modules/.bin/tape test/**/*.spec.js test/*.spec.js | tap-spec",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:watch": "npm test -- --watch",
    "test:features": "npm run build && babel-node $(npm bin)/cucumber.js",
    "test:ci": "npm run clean && cross-env NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/tape -- test/**/*.js"
  },
  "main": "./dist/sphere",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-cli.git"
  },
  "files": [
    "dist/",
    "bin/"
  ],
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-cli/issues"
  },
  "keywords": [
    "cli",
    "sphere",
    "sphere.io",
    "stream",
    "import",
    "export"
  ],
  "engines": {
    "node": ">= 0.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "./scripts/lint-staged-files && npm test"
    }
  },
  "dependencies": {
    "JSONStream": "1.0.7",
    "babel-runtime": "5.8.20",
    "commander": "emmenko/commander.js#patch-help",
    "sphere-customer-import": "1.0.4",
    "debug": "2.2.0",
    "highland": "2.5.1",
    "inflection": "1.8.0",
    "sphere-category-sync": "~0.2.6",
    "sphere-node-sdk": "^1.14.0",
    "sphere-node-utils": "0.7.0",
    "sphere-product-import": "~0.11.1",
    "sphere-product-type-import": "1.0.0",
    "sphere-stock-import": "~0.7.5",
    "stream-transform": "0.1.1",
    "winston": "1.0.1"
  },
  "eslintConfig": {
    "extends": "commercetools"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cross-env": "^3.0.0",
    "cucumber": "^0.9.2",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-commercetools": "^4.0.1",
    "eslint-formatter-pretty": "^1.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "expect": "^1.13.0",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "mversion": "^1.10.1",
    "nyc": "^8.3.0",
    "rewire": "^2.5.1",
    "rimraf": "^2.5.4",
    "should": "^7.1.0",
    "sinon": "^1.17.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "validate-commit-msg": "^2.8.0"
  }
}
